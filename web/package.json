{
  "name": "willcall-sapper",
  "description": "willcall-sapper",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev  & yarn watch:sass",
    "build": "sapper build --legacy && yarn build:sass",
    "build:sass": "node-sass src/style/global.scss static/global.css --style compressed",
    "watch:sass": "yarn build:sass && node-sass --watch src/style/global.scss static/global.css --style compressed",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "dependencies": {
    "@sanity/client": "^2.0.9",
    "@sanity/image-url": "^0.140.19",
    "body-scroll-lock": "^3.1.5",
    "compression": "^1.7.1",
    "gsap": "^3.5.1",
    "polka": "next",
    "shopify-buy": "^2.11.0",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@movingbrands/svelte-portable-text": "^0.0.11",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-url": "^5.0.0",
    "@sanity/block-content-to-hyperscript": "^2.0.10",
    "@tsconfig/svelte": "^1.0.10",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "@types/shopify-buy": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "node-sass": "^5.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sanity-shopify-toolkit": "^0.1.6",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.46",
    "svelte-preprocess": "^4.6.1",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  }
}
